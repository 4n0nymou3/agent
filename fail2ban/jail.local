[sshd]
enabled  = true
port     = ssh
filter   = sshd
logpath  = /var/log/auth.log
maxretry = 3

[nginx-http-auth]
enabled  = true
port     = http,https
filter   = nginx-http-auth
logpath  = /var/log/nginx/error.log
maxretry = 3

[xray]
enabled  = true
port     = 443,2053,8443
filter   = xray
logpath  = /var/log/xray_access.log
maxretry = 3
